div(id="tour_Memory" class="panel panel-default" style="max-height:300px; overflow: scroll;")
  div(class="panel-body")
    div(class="memory-block" ng-repeat="value in memory.data track by $index" style="width:3em;display:inline-block;text-align:center;")
      span(class="label label-default" ng-class="{'label-info':$index == currentAddress}")
        | {{ value | memoryfilter}}
        
    a(class="close pull-right" aria-hidden="true")
      span(class="fa fa-question-circle" aria-hidden="true")
